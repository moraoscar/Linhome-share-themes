	<!-- Gradient color gradient-color.<key in app> -->

	<section name="gradient-color.dark_light_vertical_gradient"> <!--Used by launch screen -->
		<entry name="from">bg</entry>
		<entry name="to">bg</entry>
		<entry name="orientation">top_bottom</entry> <!--Supported values are : top_bottom (default), bottom_top, right_left, left_right -->
	</section>


	<!-- Button effects DEFAULT/SELECTED/DISABLED selection-effect. One color for default state, one color for selected state <key in app> -->

	<section name="selection-effect.sidemenu_option">
		<entry name="default">color_b</entry>
		<entry name="selected">color_a</entry>
	</section>

	<section name="selection-effect.dropdown_list">
		<entry name="default">color_d</entry>
		<entry name="selected">color_i</entry>
	</section>

	<section name="selection-effect.settings_item">
		<entry name="default">color_c</entry>
		<entry name="selected">color_h</entry>
	</section>

    <section name="selection-effect.white">
        <entry name="default">color_c</entry>
        <entry name="selected">color_c</entry>
        <entry name="disabled">color_c</entry>
    </section>
    
	<section name="selection-effect.primary_color">
		<entry name="default">primary</entry>
		<entry name="selected">primary</entry>
		<entry name="disabled">primary</entry>
	</section>

